<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zestzone.constraint">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PlaceHolderActivity"></activity>
        <activity android:name=".BarrierActivity" />
        <activity android:name=".GuideLineActivity" />
        <activity android:name=".GroupActivity" />
        <activity android:name=".OptimizerActivity" />
        <activity android:name=".ChainActivity" />
        <activity android:name=".GoneMarginActivity" />
        <activity android:name=".PercentActivity" />
        <activity android:name=".CircularPositionActivity" />
        <activity android:name=".RelativePositionActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>